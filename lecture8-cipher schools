//for loop
class HelloWorld {
    public static void main(String[] args) {
        for(int i=0;i<=10;i++){

            System.out.println(i);
        }
    }
}


//while loop
class HelloWorld {
    public static void main(String[] args) {
        int i=0;
        while(i<=10){
            i++;
            System.out.println(i);
        }
    }
}
